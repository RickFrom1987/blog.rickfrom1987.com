<!-- build:js public/js/site.js -->
<script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
<script type="text/javascript" src="<%- config.root%>js/jquery.fitvids.js"></script>
<script type="text/javascript" src="<%- config.root%>js/jquery.lazyload.js"></script>
<script src="<%- config.root %>js/jquery.fancybox.js"></script>
<script type="text/javascript" src="<%- config.root%>js/index.js?v=<%- config.version %>"></script>
<!-- endbuild -->
<% if (config.disqus_shortname){ %>
<script type="text/javascript">
    var disqus_shortname = '<%= config.disqus_shortname %>';

    (function(){
        var dsq = document.createElement('script');
        dsq.type = 'text/javascript';
        dsq.async = true;
        dsq.src = '//' + disqus_shortname + '.disqus.com/<% if (page.comments){ %>embed.js<% } else { %>count.js<% } %>';
        (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
    }());
</script>
<% } %>
